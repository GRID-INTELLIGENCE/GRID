{
  "version": "1.0.0",
  "description": "Wellness Studio AI Safety Denylist Configuration",
  "globalSettings": {
    "defaultAction": "allow",
    "enforcementMode": "strict",
    "logDenylistEvents": true,
    "phiRedaction": true,
    "hipaaCompliance": true
  },
  "denylistRules": [
    {
      "name": "disable-network-dependent-clinical",
      "reason": "network-dependent",
      "scope": "global",
      "enabled": true,
      "matchAttributes": {
        "category": ["web-based", "api-gateway"],
        "requiresNetwork": true
      },
      "notes": "Disable network-dependent servers for clinical safety"
    },
    {
      "name": "disable-failed-python-spawns",
      "reason": "startup-failure",
      "scope": "global",
      "enabled": true,
      "matchAttributes": {
        "commandPattern": "^python$"
      },
      "notes": "Disable servers with Python spawn errors"
    },
    {
      "name": "disable-high-resource-during-analysis",
      "reason": "resource-intensive",
      "scope": "temporary",
      "enabled": false,
      "matchAttributes": {
        "resourceProfile": ["high", "critical"]
      },
      "notes": "Temporarily disable during patient analysis (enable when needed)"
    }
  ],
  "serverInventory": [
    {
      "name": "patient-analyzer",
      "category": "patient-data-processor",
      "command": "python",
      "requiresNetwork": false,
      "requiresDatabase": true,
      "port": null,
      "dependencies": ["python", "pandas", "scikit-learn"],
      "resourceProfile": "medium",
      "priority": 9
    },
    {
      "name": "wellness-recommender",
      "category": "clinical-decision-support",
      "command": "python",
      "requiresNetwork": false,
      "requiresDatabase": false,
      "port": null,
      "dependencies": ["python"],
      "resourceProfile": "low",
      "priority": 8
    }
  ],
  "protectedPaths": {
    "immutable": true,
    "paths": [
      "E:\\_projects\\wellness_studio",
      "E:\\_projects\\grid",
      "E:\\_projects\\Coinbase\\coinbase"
    ],
    "notes": "These project paths are protected and must not be relocated"
  }
}
