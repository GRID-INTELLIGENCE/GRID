{
  "python.analysis.extraPaths": [
    "./grid/src",
    "./projects/EUFLE/eufle",
    "./projects/pipeline",
    "./projects/echoes",
    "./projects/wonderous-structures",
    "./projects/Apps",
    "./shared/workspace_utils"
  ],
  "python.analysis.typeCheckingMode": "basic",
  "python.analysis.diagnosticMode": "workspace",
  "python.testing.pytestEnabled": true,
  "python.testing.pytestArgs": [
    "grid/tests",
    "projects/EUFLE/tests",
    "shared/workspace_utils/tests",
    "-v"
  ],
  "terminal.integrated.cwd": "${workspaceFolder}",
  "terminal.integrated.shellIntegration.enabled": false,
  "terminal.integrated.shellIntegration.decorationsEnabled": "never",
  "terminal.integrated.env.linux": {
    "PROJECT_ROOT": "${workspaceFolder}",
    "GRID_HOME": "${workspaceFolder}/grid",
    "EUFLE_HOME": "${workspaceFolder}/projects/EUFLE",
    "EUFLE_ROOT": "${workspaceFolder}/projects/EUFLE",
    "ENV": "development",
    "SAMPLE_RATE": "48000",
    "BPM": "124",
    "BIT_DEPTH": "32_float"
  },
  "terminal.integrated.env.windows": {
    "PROJECT_ROOT": "${workspaceFolder}",
    "GRID_HOME": "${workspaceFolder}\\grid",
    "EUFLE_HOME": "${workspaceFolder}\\projects\\EUFLE",
    "EUFLE_ROOT": "${workspaceFolder}\\projects\\EUFLE",
    "ENV": "development",
    "SAMPLE_RATE": "48000",
    "BPM": "124",
    "BIT_DEPTH": "32_float"
  },
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": "explicit",
    "source.organizeImports": "explicit"
  },
  "eslint.workingDirectories": [
    "./projects/Apps",
    "./grid"
  ],
  "editor.defaultFormatter": null,
  "[python]": {
    "editor.defaultFormatter": null
  },
  "search.exclude": {
    "**/node_modules": true,
    "**/__pycache__": true,
    "**/.pytest_cache": true,
    "**/.mypy_cache": true,
    "**/.ruff_cache": true,
    "**/.cache": true,
    "**/.ipynb_checkpoints": true,
    "**/site-packages": true,
    "**/analysis_outputs": true,
    "**/dist": true,
    "**/build": true,
    "**/.venv": true,
    "**/venv": true,
    "**/artifacts": true,
    "**/logs": true,
    "**/*.pyc": true,
    "**/*.vhd": true,
    "**/*.vhdx": true,
    "DevDrive.vhdx": true,
    "VHD.vhdx": true
  },
  "files.watcherExclude": {
    "**/.git/**": true,
    "**/node_modules/**": true,
    "**/__pycache__/**": true,
    "**/.pytest_cache/**": true,
    "**/.mypy_cache/**": true,
    "**/.ruff_cache/**": true,
    "**/.cache/**": true,
    "**/.ipynb_checkpoints/**": true,
    "**/site-packages/**": true,
    "**/analysis_outputs/**": true,
    "**/dist/**": true,
    "**/build/**": true,
    "**/.venv/**": true,
    "**/venv/**": true,
    "**/artifacts/**": true,
    "**/logs/**": true
  },
  "files.exclude": {
    "**/__pycache__": true,
    "**/.pytest_cache": true,
    "**/.mypy_cache": true,
    "**/.ruff_cache": true,
    "**/*.pyc": true,
    "**/.DS_Store": true
  },
  "audio-team.config": {
    "bpm": 124,
    "sample_rate": 48000,
    "bit_depth": "32_float",
    "modules": [
      "kick",
      "drums",
      "percussion",
      "bass",
      "synths",
      "arps",
      "atmos",
      "fx"
    ]
  },
  "workbench.editor.alwaysShowEditorActions": true,
  "workbench.editor.autoLockGroups": {
    "default": true,
    "workbench.editorinputs.searchEditorInput": true,
    "workbench.input.interactive": true,
    "jsProfileVisualizer.heapsnapshot.table": true,
    "jsProfileVisualizer.heapprofile.table": true,
    "jsProfileVisualizer.cpuprofile.table": true,
    "vscode.videoPreview": true,
    "vscode.audioPreview": true,
    "imagePreview.previewEditor": true,
    "workbench.editors.gettingStartedInput": true,
    "repl": true,
    "jupyter-notebook": true,
    "notebookOutputEditor": true
  },
  "workbench.editor.closeOnFileDelete": true,
  "workbench.editor.centeredLayoutFixedWidth": true,
  "terminal.integrated.profiles.windows": {
    "PowerShell": {
      "source": "PowerShell",
      "icon": "terminal-powershell"
    },
    "WSL": {
      "path": "wsl.exe",
      "args": [
        "-d",
        "Ubuntu"
      ],
      "icon": "terminal-linux",
      "color": "terminal.ansiBlue"
    }
  },
  "terminal.integrated.defaultProfile.windows": "PowerShell",
  "cursorpyright.analysis.diagnosticMode": "workspace",
  "cursorpyright.analysis.extraPaths": [
    "./grid/src",
    "./EUFLE/eufle",
    "./pipeline",
    "./echoes",
    "./wonderous-structures",
    "./Apps",
    "./workspace_utils"
  ],
  "cursorpyright.analysis.typeCheckingMode": "basic",
  "python.languageServer": "None",
  "go.useLanguageServer": false,
  "go.toolsManagement.autoUpdate": false,
  "go.buildOnSave": "off",
  "go.lintOnSave": "off",
  "go.vetOnSave": "off",
  "go.testOnSave": "off",
  "go.survey.prompt": false,
  "go.welcome.show": false,
  "cSpell.words": [
    "compresslevel",
    "fastapi"
  ],
  "chat.agent.maxRequests": 55,
  "chat.customAgentInSubagent.enabled": true,
  "chat.editor.wordWrap": "on",
  "accessibility.openChatEditedFiles": true,
  "chat.edits2.enabled": true,
  "chat.mcp.discovery.enabled": {
    "cursor-workspace": true,
    "cursor-global": true,
    "windsurf": true,
    "claude-desktop": true
  },
  "chat.renderRelatedFiles": true,
  "chat.restoreLastPanelSession": true,
  "chat.tools.terminal.autoReplyToPrompts": true,
  "chat.initialHint": false,
  "chat.useAgentSkills": true,
  "chat.useNestedAgentsMdFiles": true,
  "inlineChat.enableV2": true,
  "inlineChat.notebookAgent": true,
  "github.copilot.chat.agent.autoFix": true,
  "github.copilot.chat.alternateGeminiModelFPrompt.enabled": true,
  "github.copilot.chat.alternateGptPrompt.enabled": true,
  "github.copilot.chat.anthropic.tools.websearch.enabled": true,
  "github.copilot.chat.anthropic.useMessagesApi": true,
  "github.copilot.chat.codesearch.enabled": true,
  "github.copilot.chat.customAgents.showOrganizationAndEnterpriseAgents": true,
  "github.copilot.chat.generateTests.codeLens": true,
  "github.copilot.chat.githubMcpServer.enabled": true,
  "github.copilot.chat.githubMcpServer.lockdown": true,
  "github.copilot.chat.newWorkspace.useContext7": true,
  "github.copilot.chat.notebook.enhancedNextEditSuggestions.enabled": true,
  "github.copilot.chat.notebook.followCellExecution.enabled": true
}
