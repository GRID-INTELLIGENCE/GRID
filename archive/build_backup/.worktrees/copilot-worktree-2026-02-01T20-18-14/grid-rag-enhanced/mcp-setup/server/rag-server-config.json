{
  "mcpServers": {
    "grid-rag": {
      "command": "python",
      "args": [
        "mcp-setup/server/grid_rag_mcp_server.py"
      ],
      "cwd": "${workspaceFolder}",
      "env": {
        "PYTHONPATH": "${workspaceFolder}/src:${workspaceFolder}",
        "RAG_EMBEDDING_PROVIDER": "huggingface",
        "RAG_EMBEDDING_MODEL": "BAAI/bge-small-en-v1.5",
        "RAG_LLM_MODE": "ollama",
        "RAG_LLM_MODEL_OLLAMA": "qwen3-coder:480b-cloud",
        "RAG_VECTOR_STORE_PROVIDER": "chromadb",
        "RAG_VECTOR_STORE_PATH": ".rag_db",
        "OLLAMA_BASE_URL": "http://localhost:11434"
      }
    },
    "grid-enhanced-tools": {
      "command": "python",
      "args": [
        "mcp-setup/server/enhanced_tools_mcp_server.py"
      ],
      "cwd": "${workspaceFolder}",
      "env": {
        "PYTHONPATH": "${workspaceFolder}/src:${workspaceFolder}"
      }
    }
  }
}
