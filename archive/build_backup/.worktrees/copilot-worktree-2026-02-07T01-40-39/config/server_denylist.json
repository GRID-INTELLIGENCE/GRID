                                                   "filesystem",
                                                   "network",
                                                   "database",
                                                   "memory",
                                                   "system"
                                               ],
                              "enabled":  true,
                              "notes":  "Total deny scope for grid-dev-tools MCP server",
                              "name":  "deny-grid-dev-tools",
                              "scope":  "global",
                              "targetServer":  "grid-dev-tools"
                          },
                          {
                              "pattern":  "portfolio-.*",
                              "matchAttributes":  {
                                                      "category":  "portfolio"
                                                  },
                              "reason":  "mcp-server-denial",
                              "deniedScopes":  [
                                                   "filesystem",
                                                   "network",
                                                   "database",
                                                   "memory",
                                                   "system"
                                               ],
                              "enabled":  true,
                              "notes":  "Total deny scope for all portfolio-* MCP servers",
                              "name":  "deny-portfolio-servers",
                              "scope":  "global"
                          },
                          {
                              "matchAttributes":  {
                                                      "commandPattern":  "(?i).*python(?:\\.exe)?.*mcp"
                                                  },
                              "deniedScopes":  [
                                                   "subprocess",
                                                   "spawn",
                                                   "execution"
                                               ],
                              "reason":  "spawn-prevention",
                              "notes":  "Prevent Python-based MCP server spawning",
                              "name":  "deny-python-mcp-spawns",
                              "scope":  "global",
                              "enabled":  true
                          },
                          {
                              "name":  "deny-grid-rag",
                              "reason":  "mcp-server-denial",
                              "scope":  "global",
                              "enabled":  true,
                              "targetServer":  "grid-rag",
                              "notes":  "Total deny for grid-rag"
                          },
                          {
                              "name":  "deny-grid-agentic",
                              "reason":  "mcp-server-denial",
                              "scope":  "global",
                              "enabled":  true,
                              "targetServer":  "grid-agentic",
                              "notes":  "Total deny for grid-agentic"
                          },
                          {
                              "name":  "deny-grid-dev-tools",
                              "reason":  "mcp-server-denial",
                              "scope":  "global",
                              "enabled":  true,
                              "targetServer":  "grid-dev-tools",
                              "notes":  "Total deny for grid-dev-tools"
                          },
                          {
                              "name":  "deny-portfolio-servers",
                              "reason":  "mcp-server-denial",
                              "scope":  "global",
                              "enabled":  true,
                              "pattern":  "portfolio-.*",
                              "notes":  "Total deny for portfolio servers"
                          },
                          {
                              "name":  "deny-python-mcp-spawns",
                              "reason":  "spawn-prevention",
                              "scope":  "global",
                              "enabled":  true,
                              "notes":  "Prevent Python MCP spawning"
                          }
