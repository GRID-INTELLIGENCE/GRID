                              "name":  "deny-grid-agentic",
                              "scope":  "global",
                              "targetServer":  "grid-agentic"
                          },
                          {
                              "matchAttributes":  {
                                                      "name":  "grid-dev-tools"
                                                  },
                              "reason":  "mcp-server-denial",
