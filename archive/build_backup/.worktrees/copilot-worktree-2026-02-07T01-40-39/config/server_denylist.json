{
    "version":  "1.0.0",
    "globalSettings":  {
                           "defaultAction":  "allow",
                           "enforcementMode":  "strict",
                           "logDenylistEvents":  true,
                           "phiRedaction":  true,
                           "hipaaCompliance":  true
                       },
    "denylistRules":  [
                          {
                              "name":  "disable-web-servers",
                              "reason":  "network-dependent",
                              "scope":  "global",
                              "enabled":  true,
                              "matchAttributes":  {
                                                      "category":  [
                                                                       "web-based",
                                                                       "api-gateway"
                                                                   ],
                                                      "requiresNetwork":  true
                                                  },
                              "notes":  "Disable all web-based servers and API gateways to reduce network dependency"
                          },
                          {
                              "name":  "disable-high-resource-servers",
                              "reason":  "resource-intensive",
                              "scope":  "temporary",
                              "enabled":  false,
                              "matchAttributes":  {
                                                      "resourceProfile":  [
                                                                              "high",
                                                                              "critical"
                                                                          ]
                                                  },
                              "notes":  "Temporarily disable resource-intensive servers during development"
                          },
                          {
                              "name":  "disable-mcp-servers",
                              "reason":  "user-disabled",
                              "scope":  "workspace",
                              "enabled":  true,
                              "matchAttributes":  {
                                                      "category":  [
                                                                       "mcp-server"
                                                                   ]
                                                  },
                              "notes":  "User requested to disable MCP servers temporarily"
                          },
                          {
                              "name":  "disable-failed-python-spawns",
                              "reason":  "startup-failure",
                              "scope":  "global",
                              "enabled":  true,
                              "pattern":  ".*",
                              "matchAttributes":  {
                                                      "commandPattern":  "(?i)^(python|py)$"
                                                  },
                              "notes":  "Disable servers with python spawn errors (ENOENT)"
                          },
                          {
                              "matchAttributes":  {
                                                      "name":  "grid-rag"
                                                  },
                              "reason":  "mcp-server-denial",
                              "deniedScopes":  [
                                                   "filesystem",
                                                   "network",
                                                   "database",
                                                   "memory",
                                                   "system"
                                               ],
                              "enabled":  true,
                              "notes":  "Total deny scope for grid-rag MCP server across all editors",
                              "name":  "deny-grid-rag",
                              "scope":  "global",
                              "targetServer":  "grid-rag"
                          },
                          {
                              "matchAttributes":  {
                                                      "name":  "grid-agentic"
                                                  },
                              "reason":  "mcp-server-denial",
