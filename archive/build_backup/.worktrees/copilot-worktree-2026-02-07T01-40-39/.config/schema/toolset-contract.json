{
  "toolsetContract": {
    "version": "1.0.0",
    "schema": "grid-toolset-v1",
    "description": "Machine-readable configuration contract for GRID tool ecosystem",
    "lastUpdated": "2026-01-24T14:32:00Z",
    "toolsets": {
      "coreIntelligence": {
        "tools": [
          "grid-analyze",
          "grid-process",
          "grid-skills",
          "grid-storytime",
          "grid-serve"
        ],
        "description": "Core GRID intelligence and analysis capabilities",
        "icon": "brain",
        "category": "intelligence",
        "dependencies": ["python>=3.13", "fastapi>=0.104.0"],
        "entryPoints": {
          "cli": "src.grid.__main__:main",
          "api": "src.grid.entry_points.api_entry:main",
          "service": "src.grid.entry_points.service_entry:main"
        },
        "capabilities": [
          "text-analysis",
          "entity-extraction",
          "relationship-mapping",
          "skill-execution",
          "story-generation"
        ],
        "configSchema": {
          "confidence": {
            "type": "float",
            "default": 0.7,
            "range": [0.0, 1.0]
          },
          "max_entities": {
            "type": "int",
            "default": 0,
            "min": 0
          },
          "use_rag": {
            "type": "boolean",
            "default": false
          },
          "output_format": {
            "type": "enum",
            "values": ["json", "table", "yaml"],
            "default": "table"
          }
        }
      },
      "ragSystem": {
        "tools": [
          "rag-index",
          "rag-query",
          "rag-ondemand",
          "rag-evaluate",
          "rag-stats"
        ],
        "description": "Retrieval-Augmented Generation system for knowledge management",
        "icon": "search",
        "category": "knowledge",
        "dependencies": ["sentence-transformers", "chromadb", "ollama"],
        "entryPoints": {
          "cli": "src.tools.rag.cli:main"
        },
        "capabilities": [
          "vector-indexing",
          "semantic-search",
          "hybrid-search",
          "cross-encoder-reranking",
          "on-demand-processing"
        ],
        "configSchema": {
          "embedding_provider": {
            "type": "enum",
            "values": ["huggingface", "ollama"],
            "default": "huggingface"
          },
          "embedding_model": {
            "type": "string",
            "default": "all-MiniLM-L6-v2"
          },
          "llm_provider": {
            "type": "enum",
            "values": ["ollama", "openai"],
            "default": "ollama"
          },
          "top_k": {
            "type": "int",
            "default": 5,
            "min": 1,
            "max": 50
          },
          "temperature": {
            "type": "float",
            "default": 0.7,
            "range": [0.0, 2.0]
          },
          "use_hybrid": {
            "type": "boolean",
            "default": false
          },
          "use_reranker": {
            "type": "boolean",
            "default": false
          }
        }
      },
      "architecturalTools": {
        "tools": [
          "architectural-audit",
          "inventory",
          "integration-test"
        ],
        "description": "Code architecture analysis and compliance tools",
        "icon": "architecture",
        "category": "development",
        "dependencies": ["ast", "pathlib", "networkx"],
        "entryPoints": {
          "audit": "src.tools.architectural_audit:ArchitecturalDivergenceAnalyzer",
          "inventory": "src.tools.inventory",
          "integration": "src.tools.test_interfaces_integration"
        },
        "capabilities": [
          "divergence-analysis",
          "pattern-detection",
          "migration-path-generation",
          "compliance-checking"
        ],
        "configSchema": {
          "mothership_pattern": {
            "type": "boolean",
            "default": true
          },
          "divergence_threshold": {
            "type": "float",
            "default": 0.4,
            "range": [0.0, 1.0]
          },
          "auto_migration": {
            "type": "boolean",
            "default": false
          },
          "reference_modules": {
            "type": "array",
            "default": ["application/mothership"]
          }
        }
      },
      "codeQuality": {
        "tools": [
          "ruff-check",
          "ruff-format",
          "mypy-analysis",
          "black-format"
        ],
        "description": "Code formatting, linting, and type checking",
        "icon": "code",
        "category": "quality",
        "dependencies": ["ruff>=0.0.291", "mypy>=1.5.1", "black>=23.9.0"],
        "entryPoints": {
          "ruff": "python -m ruff",
          "mypy": "python -m mypy",
          "black": "python -m black"
        },
        "capabilities": [
          "linting",
          "formatting",
          "type-checking",
          "import-sorting",
          "complexity-analysis"
        ],
        "configSchema": {
          "line_length": {
            "type": "int",
            "default": 120,
            "min": 80,
            "max": 200
          },
          "target_version": {
            "type": "string",
            "default": "py313"
          },
          "strict_type_checking": {
            "type": "boolean",
            "default": false
          },
          "format_on_save": {
            "type": "boolean",
            "default": true
          }
        }
      },
      "systemMonitoring": {
        "tools": [
          "pulse-monitor",
          "interfaces-dashboard",
          "health-check",
          "zoology-mapper"
        ],
        "description": "System health monitoring and ambient intelligence",
        "icon": "activity",
        "category": "monitoring",
        "dependencies": ["psutil", "asyncio", "websockets"],
        "entryPoints": {
          "pulse": "src.tools.pulse_monitor",
          "dashboard": "src.tools.interfaces_dashboard",
          "health": "src.tools.health_check",
          "zoology": "src.tools.zoology_mapper"
        },
        "capabilities": [
          "system-pulse",
          "interface-metrics",
          "health-analysis",
          "component-mapping"
        ],
        "configSchema": {
          "monitor_interval": {
            "type": "int",
            "default": 30,
            "min": 1,
            "max": 300
          },
          "alert_threshold": {
            "type": "float",
            "default": 0.8,
            "range": [0.0, 1.0]
          },
          "ambient_enabled": {
            "type": "boolean",
            "default": false
          },
          "zoology_depth": {
            "type": "int",
            "default": 3,
            "min": 1,
            "max": 10
          }
        }
      },
      "llmOperations": {
        "tools": [
          "code-transformer",
          "model-router",
          "fine-tuning-pipeline",
          "inference-engine"
        ],
        "description": "LLM operations, code transformation, and model routing",
        "icon": "cpu",
        "category": "intelligence",
        "dependencies": ["transformers", "torch", "peft"],
        "entryPoints": {
          "cli": "EUFLE.eufle:main",
          "transformer": "EUFLE.studio.code_transformer",
          "router": "EUFLE.studio.model_router"
        },
        "capabilities": [
          "code-transformation",
          "language-translation",
          "model-routing",
          "fine-tuning",
          "inference-optimization"
        ],
        "configSchema": {
          "default_provider": {
            "type": "enum",
            "values": ["ollama", "openai", "anthropic"],
            "default": "ollama"
          },
          "default_model": {
            "type": "string",
            "default": "mistral-nemo:latest"
          },
          "fine_tune_method": {
            "type": "enum",
            "values": ["lora", "full", "adapter"],
            "default": "lora"
          },
          "precision": {
            "type": "enum",
            "values": ["float32", "float16", "bfloat16"],
            "default": "bfloat16"
          }
        }
      },
      "dataPipeline": {
        "tools": [
          "ocr-extractor",
          "deduplication",
          "cascade-classifier",
          "output-formatter"
        ],
        "description": "Data processing pipeline: OCR, dedup, ML cascade classification",
        "icon": "package",
        "category": "data",
        "dependencies": ["opencv-python", "pytesseract", "pandas"],
        "entryPoints": {
          "cli": "pipeline.main:main",
          "service": "pipeline.services"
        },
        "capabilities": [
          "ocr-extraction",
          "deduplication",
          "keyword-classification",
          "ml-classification",
          "llm-fallback",
          "parquet-output"
        ],
        "configSchema": {
          "ocr_engine": {
            "type": "enum",
            "values": ["tesseract", "paddleocr"],
            "default": "tesseract"
          },
          "dedup_threshold": {
            "type": "float",
            "default": 0.95,
            "range": [0.0, 1.0]
          },
          "cascade_order": {
            "type": "array",
            "default": ["keyword", "ml", "llm"]
          },
          "output_format": {
            "type": "enum",
            "values": ["parquet", "json", "csv"],
            "default": "parquet"
          }
        }
      },
      "workspaceUtils": {
        "tools": [
          "repo-analyzer",
          "project-comparator",
          "entity-classifier",
          "eufle-verifier"
        ],
        "description": "Workspace utilities for repository analysis and verification",
        "icon": "folder-open",
        "category": "development",
        "dependencies": ["click", "pydantic", "pathlib"],
        "entryPoints": {
          "cli": "workspace_utils.cli:main",
          "analyzer": "workspace_utils.repo_analyzer:RepositoryAnalyzer"
        },
        "capabilities": [
          "static-analysis",
          "dependency-extraction",
          "module-mapping",
          "project-comparison",
          "setup-verification"
        ],
        "configSchema": {
          "output_format": {
            "type": "enum",
            "values": ["json", "markdown", "table"],
            "default": "json"
          },
          "include_metrics": {
            "type": "boolean",
            "default": true
          },
          "recursive": {
            "type": "boolean",
            "default": true
          }
        }
      },
      "developmentAutomation": {
        "tools": [
          "daily-harvest",
          "config-sync",
          "session-tracker",
          "checkpoint-manager"
        ],
        "description": "Development automation and workflow management",
        "icon": "calendar",
        "category": "productivity",
        "dependencies": ["powershell>=5.0", "python>=3.11"],
        "entryPoints": {
          "harvest": "daily_harvest.ps1",
          "sync": ".editor-config/sync/config-sync.ps1",
          "checkpoint": ".checkpoints.json"
        },
        "capabilities": [
          "scheduled-analysis",
          "config-sync",
          "session-logging",
          "progress-tracking"
        ],
        "configSchema": {
          "harvest_schedule": {
            "type": "enum",
            "values": ["hourly", "daily", "weekly"],
            "default": "daily"
          },
          "sync_bidirectional": {
            "type": "boolean",
            "default": true
          },
          "auto_checkpoint": {
            "type": "boolean",
            "default": true
          },
          "retention_days": {
            "type": "int",
            "default": 30,
            "min": 7
          }
        }
      },
      "aiCollaboration": {
        "tools": [
          "cursor-rules",
          "module-discovery",
          "architecture-explorer",
          "context-bridge"
        ],
        "description": "AI tool integration and context passing",
        "icon": "sparkles",
        "category": "intelligence",
        "dependencies": ["vscode", "windsurf"],
        "entryPoints": {
          "rules": ".cursorrules",
          "discovery": "GET /api/config/repos",
          "architecture": ".config/schema/architecture.json"
        },
        "capabilities": [
          "ai-context-injection",
          "smart-autocompletion",
          "architecture-navigation",
          "cross-project-awareness"
        ],
        "configSchema": {
          "enable_windsurf": {
            "type": "boolean",
            "default": true
          },
          "enable_vscode": {
            "type": "boolean",
            "default": true
          },
          "context_size": {
            "type": "enum",
            "values": ["fixed", "adaptive"],
            "default": "adaptive"
          },
          "auto_refresh": {
            "type": "boolean",
            "default": true
          }
        }
      }
    },
    "globalConfig": {
      "environment": {
        "GRID_SESSIONS": "logs/sessions",
        "GRID_CONVERSATIONS": "logs/conversations",
        "GRID_ROOT": "${workspaceFolder}",
        "PYTHONPATH": "${workspaceFolder}/src:${workspaceFolder}"
      },
      "paths": {
        "logs": "logs/",
        "cache": ".cache/",
        "temp": "temp/",
        "output": "output/"
      },
      "security": {
        "require_auth": false,
        "token_expiry": 3600,
        "max_concurrent_sessions": 10
      },
      "performance": {
        "max_workers": 4,
        "timeout_default": 30,
        "cache_ttl": 3600
      }
    },
    "integrationMatrix": {
      "coreIntelligence": ["ragSystem", "aiCollaboration"],
      "ragSystem": ["coreIntelligence", "developmentAutomation"],
      "architecturalTools": ["codeQuality", "workspaceUtils"],
      "codeQuality": ["systemMonitoring", "architecturalTools"],
      "systemMonitoring": ["developmentAutomation"],
      "llmOperations": ["dataPipeline", "codeQuality"],
      "dataPipeline": ["llmOperations", "workspaceUtils"],
      "workspaceUtils": ["architecturalTools", "dataPipeline"],
      "developmentAutomation": ["systemMonitoring", "aiCollaboration"],
      "aiCollaboration": ["coreIntelligence", "ragSystem", "developmentAutomation"]
    }
  },
  "configContract": {
    "name": "GRID Toolset Configuration Contract",
    "version": "1.0.0",
    "effectiveDate": "2026-01-24T14:32:00Z",
    "parties": {
      "provider": "GRID System",
      "consumer": "VS Code Workspace",
      "arbiter": "Configuration Schema"
    },
    "terms": {
      "validation": "strict_schema_compliance",
      "configurationBinding": "declarative_runtime_validated",
      "accessControl": "environment_variable_bypass_available",
      "performanceGuarantees": {
        "response_time": "<5s for CLI operations",
        "memory_usage": "<2GB for standard operations",
        "concurrent_users": "up to 10",
        "availability": "99% during business hours"
      },
      "updateMechanism": "semantic_versioning_with_backward_compatibility"
    }
  }
}
